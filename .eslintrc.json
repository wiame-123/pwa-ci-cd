{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "overrides": [
      {
        "files": ["**/*.test.js"],
        "env": {
          "jest": true
        }
      }
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "eqeqeq": "warn",
      "semi": ["warn", "always"]
    }
  }
  